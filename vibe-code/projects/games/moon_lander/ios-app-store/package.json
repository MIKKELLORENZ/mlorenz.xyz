{
  "name": "moon-lander-ios",
  "version": "1.0.0",
  "description": "Retro Moon Lander Game - iOS App Store Version",
  "main": "index.js",
  "scripts": {
    "build": "echo 'Build complete'",
    "cap:init": "npx cap init \"Moon Lander\" \"xyz.mlorenz.moonlander\" --web-dir www",
    "cap:add:ios": "npx cap add ios",
    "cap:copy": "npx cap copy ios",
    "cap:sync": "npx cap sync ios",
    "cap:open": "npx cap open ios",
    "prepare-ios": "npm run cap:copy && npm run cap:sync",
    "open-xcode": "npm run cap:open"
  },
  "keywords": [
    "game",
    "moon-lander",
    "ios",
    "capacitor"
  ],
  "author": "Mikkel Vind Lorenz",
  "license": "MIT",
  "dependencies": {
    "@capacitor/app": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/haptics": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/status-bar": "^6.0.0",
    "@capacitor/splash-screen": "^6.0.0",
    "@capacitor/preferences": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0"
  }
}
